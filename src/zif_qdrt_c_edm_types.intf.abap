"! <p class="shorttext synchronized" lang="en">Constants of EDM Data Types</p>
INTERFACE zif_qdrt_c_edm_types
  PUBLIC.

  CONSTANTS:
    string           TYPE string VALUE 'String',
    decimal          TYPE string VALUE 'Decimal',
    byte             TYPE string VALUE 'Byte',
    float            TYPE string VALUE 'Float',
    double           TYPE string VALUE 'Double',
    int16            TYPE string VALUE 'Int16',
    int32            TYPE string VALUE 'Int32',
    int64            TYPE string VALUE 'Int64',
    binary           TYPE string VALUE 'Binary',
    time             TYPE string VALUE 'Time',
    date_time        TYPE string VALUE 'DateTime',
    date_time_offset TYPE string VALUE 'DateTimeOffset',
    date             TYPE string VALUE 'Date',
    guid             TYPE string VALUE 'Guid',
    boolean          TYPE string VALUE 'Boolean'.
ENDINTERFACE.
